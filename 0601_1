using System;

namespace Enum3
{
    class MainApp
    {
        static void Main(string[] args)
        {
            Console.Write("정수를 입력하세요 : ");
            string input = Console.ReadLine();
            
            int number = Int32.Parse(input);
            
            if(number % 2 == 0 || number % 3 == 0)
            {
                Console.WriteLine("O");
            }
            else
            {
                Console.WriteLine("X");
            }
        }
    }
}
