using System;

namespace Enum3
{
    class MainApp
    {
        static void Main(string[] args)
        {
            Console.WriteLine("--------------");
            for (int i = 2; i < 10; i++)
            {
                Console.WriteLine($"\n     {i}ë‹¨\n");
                for (int k = 1; k < 10; k++)
                {
                    Console.WriteLine($"  {i} * {k} = {i * k}");
                }
                Console.WriteLine("\n--------------");
            }
        }
    }
}
