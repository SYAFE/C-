using System;

namespace Enum3
{
    class MainApp
    {
        static double Carc(double a)
        {
            return a * a;
        }
        
        static void Main(string[] args)
        {
            Console.Write("실수를 입력해주세요 : ");
            double num = double.Parse(Console.ReadLine());

            Console.WriteLine(Carc(num));
        }
    }
}
